package com.yinghai.macao.common.model;

import java.util.Date;

public class ThirdParty {
	/**
	 * 乘客类型
	 */
	public final static int USER_TYPE_SPCARPASSENGER=0;
	/**
	 * 司机类型
	 */
	public final static int USER_TYPE_DRIVER=1;
	
	/**
	 * 专车司机类型
	 */
	public final static int USER_TYPE_SPCARDRIVER=2;
	
	/**
	 * ID 唯一标识
	 */
	private Integer id;
	/**
	 * 微信登录的唯一标识
	 */
	private String openId;
	/**
	 * 第三方登录类型：1QQ，2微信，3邮箱
	 */
	private Integer type;

	/**
	 * 与用户表绑定ID
	 */
	private Integer userId;
	/**
	 * 创建时间
	 */
	private Date createTime;
	
	/**
	 * 性别
	 */
	private Integer sex;
	/**
	 *  用户类型：0为乘客，1为司机
	 */
	private Integer userType;
	/**
	 * 第三方昵称
	 */
	private String nickName;
	/**
	 * 头像
	 * @return
	 */
	private String headImg;
	
	public String getNickName() {
		return nickName;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	public String getHeadImg() {
		return headImg;
	}
	public void setHeadImg(String headImg) {
		this.headImg = headImg;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getOpenId() {
		return openId;
	}
	public void setOpenId(String openId) {
		this.openId = openId;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public Integer getUserId() {
		return userId;
	}
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Integer getSex() {
		return sex;
	}
	public void setSex(Integer sex) {
		this.sex = sex;
	}
	public Integer getUserType() {
		return userType;
	}
	public void setUserType(Integer userType) {
		this.userType = userType;
	}
	@Override
	public String toString() {
		return "ThirdParty [id=" + id + ", openId=" + openId + ", type=" + type + ", userId=" + userId + ", createTime="
				+ createTime + ", sex=" + sex + ", userType=" + userType + ", nickName=" + nickName + ", headImg="
				+ headImg + "]";
	}
	
	
}
